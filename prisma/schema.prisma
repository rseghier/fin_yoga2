generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Studio {
  id        Int     @id @default(autoincrement())
  title     String
  category  String?
  city      String?
  address   String?
  latitude  Float
  longitude Float
  placeId   String   @unique
}

